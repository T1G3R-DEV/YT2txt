{
    "manifest_version": 3,
    "name": "YT-NP2txt",
    "description": "Writes currently playing yt video/song to a file \n More info/usage: https://github.com/T1G3R-DEV/YT2txt/",
    "version": "0.4.3",
    "icons": {
        "16": "icon_16.png",
        "32": "icon_32.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    },
    "content_scripts": [{
        "matches":  ["http://www.youtube.com/*", "https://www.youtube.com/*"],
        "js": ["cScriptRData.js"]
    }],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      },
    "permissions": [ "storage", "webRequest"]
      
}